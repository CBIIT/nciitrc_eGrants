<!--start audit_report template-->
    <div class="dashboardItem">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <table width="100%">
                    <tr>
                        <td width="99%"><center>Audit Report</center></td>
                        <td width="1%">
                            <img width="14" height="14" border="0" hspace="2" src="~/Content/images/question_mark_40.jpg" title="help notice" onClick="JavaScript:help_me('audit_report')"/>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="panel-body">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th><label for="file name">File Name</label></th>
                            <th><label for="run date">Run Date</label></th>
                        </tr>
                    </thead>
                    <tbody>
                    @foreach (var auditreport in ViewBag.AuditReport)
                    {
                        <tr>
                            <td>
                                <a target="_new" title="show report" href="@Convert.ToString(Session["ImageServer"])@auditreport.report_url">@auditreport.report_name</a>
                            </td>
                            <td>@auditreport.run_date</td>
                        </tr>
                    }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
<!--end average_processing template-->