@{
    ViewBag.Title = "egrants [GPMAT Workload Report]";
    Layout = "~/Views/Shared/_egrants_header.cshtml";
}

<div id="main"><!--start main-->
    <div>
        @Html.Partial("~/Views/Egrants_Admin/_AdminMenuIndex.cshtml")
    </div>
    <div id="caption">
        <b id="page_title">GPMAT Workload Report</b>
    </div>
    Column Header Index: B=Branch; T=Team; _C=_CNT; _R=_REL; _W=_WRKLD;<br/>
    @if (ViewBag.Reports != null)
    {
    var Reports = new WebGrid(ViewBag.Reports, rowsPerPage: 20);

