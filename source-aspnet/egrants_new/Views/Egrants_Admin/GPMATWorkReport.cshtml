        footerStyle: "webgrid-footer",
        rowStyle: "webgrid-odd-row-style",
        alternatingRowStyle: "webgrid-even-row-style",
        selectedRowStyle: "webgrid-selected-row",
        numericLinksCount: 10,

        columns: Reports.Columns(
            Reports.Column("specialist_name", "Name", null, style: "col-exlarg", canSort: true),
            Reports.Column("specialist_code", "Code", null, style: "col-exs", canSort: true),
