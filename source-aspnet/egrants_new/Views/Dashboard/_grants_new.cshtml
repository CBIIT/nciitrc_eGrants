
<!--start grants_new template-->
<div class="dashboardItem">      
	<div class="panel panel-primary">
	<div class="panel-heading">
		<table width="100%">
			<tr>
			<td width="99%"><center>New Grants - 10 Days</center></td>
			<td width="1%">
				<img width="14" height="14" border="0" hspace="2" src="~/Content/images/question_mark_40.jpg" title="help notice" onClick="JavaScript:help_me('New_Grants')"/>
			</td>
			</tr>
		</table>	
	</div>
	<div class="panel-body">
	<table class="table table-hover">
	<thead>
           	<tr>
     		    <th><label for="grants">Grants</label></th>
                <th><label for="assigned date">Assigned Date</label></th>
          	</tr>
       	</thead>
      	<tbody>
    	@foreach(var grantsnew in ViewBag.GrantsNew)
        {
		    <tr>
          		<td>
			        <a target="_new" title="view grant year from GPMAT" href="https://i2e.nci.nih.gov/yourgrants/jsp/GrantDetails.jsp?applId=@Convert.ToString(grantsnew.appl_id)">						
			   	        @grantsnew.fgn
			        </a>
			    </td>
                <td>@grantsnew.assigned_date</td>
           	</tr>
        }
 	    </tbody>
   	</table>
  	</div>
   	<div class="panel-footer">[Note: These grants have been assigned to you in last 10 days]</div>
	</div>
</div>  
<!--end grants_new template-->