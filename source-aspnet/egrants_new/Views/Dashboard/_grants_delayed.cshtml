<!--start grants_delayed template-->
<div class="dashboardItem">
	<div class="panel panel-primary">
    <div class="panel-heading">
		<table width="100%">
			<tr>
			<td width="99%"><center>Late Grants</center></td>
			<td width="1%">
				<img width="14" height="14" border="0" hspace="2" src="~/Content/images/question_mark_40.jpg" title="help notice" onClick="JavaScript:help_me('Late_Grants')"/>
			</td>
			</tr>
		</table>	
	</div>
    <div class="panel-body">
	<table class="table table-hover">
        <thead>
           <tr>
                <th><label for="grant">Grant</label></th>
                <th><label for="status">Status</label></th>
		        <th><label for="days late">Days Late</label></th>
           </tr>
        </thead>
        <tbody>
	    @foreach(var grantsdelayed in ViewBag.GrantsDelayed)
        {
            <tr>
          		<td>
				    <a target="_new" title="view late year from GPMAT" href="https://i2e.nci.nih.gov/yourgrants/jsp/GrantDetails.jsp?applId=@Convert.ToString(grantsdelayed.appl_id)">@grantsdelayed.fgn</a>
		     	</td>
                <td>@grantsdelayed.status_code</td>
			    <td>@grantsdelayed.days_late</td>
		    </tr>
        }
        </tbody>			
	</table>
    </div>
    </div> 
</div>       		  
<!--end grants_delayed template-->